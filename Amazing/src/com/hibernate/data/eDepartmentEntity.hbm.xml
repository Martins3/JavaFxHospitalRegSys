<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hibernate.data.eDepartmentEntity" table="Department" schema="Hospital">
        <id name="num">
            <column name="num" sql-type="char(6)" length="6"/>
        </id>
        <property name="name">
            <column name="name" sql-type="char(10)" length="10"/>
        </property>
        <property name="abbr">
            <column name="abbr" sql-type="char(8)" length="8"/>
        </property>
    </class>
</hibernate-mapping>